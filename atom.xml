---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

  <title>Linux Экспедиция</title>
  <link href="http://visconte.github.com/atom.xml" rel="self"/>
  <link href="http://visconte.github.com"/>
  <updated>{{site.time | date_to_xmlschema }}</updated>
  <id>http://visconte.github.com</id>
  <author>
    <name>Владимир Иванов</name>
    <email></email>
  </author>

  {% for post in site.posts %}
  <entry>
    <title>{{ post.title }}</title>
    <link href="http://visconte.github.com{{ post.title }}"/>
    <updated>{{post.date | date_to_xmlschema }}</updated>
    <id>http://visconte.github.com{{ post.id }}</id>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}
</feed>